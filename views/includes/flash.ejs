<% if(successMsg && successMsg.length > 0) { %>
   <div class="alert alert-success alert-dismissible fade show" role="alert"> <%= successMsg[0] %>
   <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
   </div>
<% } %>
<% if(errorMsg && errorMsg.length > 0) { %>
    <div class="alert alert-danger alert-dismissible fade show" role="alert"> <%= errorMsg[0] %>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
 <% } %>